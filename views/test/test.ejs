            document.exitFullscreen()
            console.log(this.questions);
            this.endedAt = new Date();
            this.testEnded = true;
            try{
              this.endUpload();
            } catch(err){
                console.log(err);
            }
            this.stream.getTracks().forEach(function(track) {
                track.stop();
            });
            clearInterval(this.timeInterval);
            const answers = this.questions.map(q => {
                return q.answer;
