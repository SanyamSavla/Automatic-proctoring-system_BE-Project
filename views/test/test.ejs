    function calculateError(iris) {
        let sumX = -iris[0][0];
        let sumY = -iris[0][1];
        iris.slice(1,5).forEach(point => {
            sumX += point[0]/4;
            sumY += point[1]/4;
        })
       // console.log("iris",Math.pow(sumX,2)+Math.pow(sumY,2),0.5)
        return Math.pow(Math.pow(sumX,2)+Math.pow(sumY,2),0.5)
    } 
