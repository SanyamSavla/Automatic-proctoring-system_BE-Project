                    <table class="table">
                    <tr> 
                        <div class="fixed-table-container"> 
                          
                            <thead class="thead-light" style="" >
                                <th style="font-size:15px;font-weight: bold;">Images </th>
                              
                                
                              
                            </thead>
                              
                        
                    </tr>
                   
                    
                    <% for(let i=0; i < proctor.tests.length; i++) { %>  
                        <% if(proctor.tests[i].test == testid){ %>
