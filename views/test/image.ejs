                    <tr class="table-primary" style="color: black;">
                       
                       
                        <td>  
                        <img src="<%= url%>" alt="IMG-PRODUCT" height="150" width="300";>
                        <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04"
                        href="<%=url%>">
                        </td>
                       
                 
                    </tr>
                    <%});%> 
                    <%}%>
                    <%}%>
                      
                </div> 
                
                    </table>
                    <% } %>
