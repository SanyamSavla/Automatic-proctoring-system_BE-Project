<%- include('../partials/user-sidenav'); -%>

<!-- Main content -->
<div class="main-content" id="panel">
    <!-- Topnav -->
    <div class="container-fluid py-3 bg-primary top-padding" style="border-bottom:0.0625rem solid #e9ecef33 !important;" >
        <div class="row align-items-center">
            <div class="col col-lg-auto text-center text-sm-left">
                <h5 style="font-family: 'LakeShore', sans-serif" class="h2 text-white d-inline-block mb-0 custom-heading">Exam center</h5>
            </div>
            <div class="mx-auto mb-0 text-center"> 
                <% if(errorMsg && errorMsg.length > 0){ %>
                    <div class="alert alert-danger fade show " role="alert">
                    <span class="alert-text mr-2">
                        <%= errorMsg %>
                    </span>
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <% } %>

                <% if(username ){ %>
                <div class="alert alert-success fade show " role="alert">
                    <span class="alert-icon"><i class="ni ni-like-2"></i></span>
                    <span class="alert-text mr-2"><strong></strong>
                     Welcome   <%= username %> and check your logs
                    </span>
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <br>
                <% } %>
            </div>
            <!-- 
            <div class="col-right ml-auto mr-3  d-sm-block">
                <div class="media align-items-center">
                    <span class="avatar avatar-sm rounded-circle" >
                        <img alt="" src="../../Python/uploads/1234.png" style="width:max-content">
                    </span>
                    <div class="media-body  ml-2  d-none d-lg-block">
                        <h4 class="mb-0  " style="color: white;font-family:'Lobster', cursive;">Welcome
                            <b><%=user.name%></b>
                        </h4>
                    </div>
                </div>
            </div>
            -->
        </div>
    </div>
    <div class="main-content" id="panel">
    
        <div>             
             
                          
             <br>
             <br>
            <div class="container w-75 mx-auto  p-0" > 
                
                <% if(tests!=null) { %>
                    <table class="table" style="table-layout: fixed;
                    ">
                    <tr> 
                        <div class="fixed-table-container"> 
                          
                            <thead class="thead-light" style="" >
                                <th style="justify-content: center;font-size:15px;font-weight: bold;color:black"> Attempt  </th>
                                <th style="font-size:15px;font-weight: bold;color:black"> START  </th>
                                <th style="font-size:15px;font-weight: bold;color:black"> END  </th>
                                <th style="font-size:15px;font-weight: bold;"> Minute  </th>
                                <th style="font-size:15px;font-weight: bold;"> looked away </th>
                                <th style="text-align:center;justify-content: center;font-size:15px;font-weight: bold;white-space: -o-pre-wrap; 
                                word-wrap: break-word;
                                white-space: pre-wrap; 
                                white-space: -moz-pre-wrap; 
                                white-space: -pre-wrap;">More than 1 person found</th>
                                <th style="font-size:15px;font-weight: bold;">no person</th>
                                <th style="text-align:center;font-size:15px;font-weight: bold;white-space: -o-pre-wrap; 
                                word-wrap: break-word;
                                white-space: pre-wrap; 
                                white-space: -moz-pre-wrap; 
                                white-space: -pre-wrap;">Object found </th>
                                
                              
                            </thead>
                              
                        
                    </tr>
                   
                        <% for(let j=0; j < tests[0].logs.length; j++) { %> 
                            <% if(tests[0].logs[j].userId = user ){  %>
                            <% for(let i=0; i < tests[0].logs[j].logs.logs.length; i++) { %> 
                              
                    <tr class="table-primary" style="color: black;  ">
                        <td>No. <%= j %></td>
                        <td style="white-space: -o-pre-wrap; 
                        word-wrap: break-word;
                        white-space: pre-wrap; 
                        white-space: -moz-pre-wrap; 
                        white-space: -pre-wrap;"> <%= tests[0].logs[j].testStartedAt %></td>
                        <td style="white-space: -o-pre-wrap; 
                        word-wrap: break-word;
                        white-space: pre-wrap; 
                        white-space: -moz-pre-wrap; 
                        white-space: -pre-wrap;"><%= tests[0].logs[j].testCompletedAt %></td>
                        <td><%=i %></td>
                        <td><%= tests[0].logs[j].logs.logs[i][0]%></td>
                      
                        <td><%= tests[0].logs[j].logs.logs[i][1] %></td>
                        <td> <%= tests[0].logs[j].logs.logs[i][2]%> </td>
                        <td><%= tests[0].logs[j].logs.logs[i][3] %></td>
                       
                    
                    </tr>
                    <% }} %>
                    </div> 
                    <% } %>
                    </table>
                   
            </div>
            <% } %>
        </div>
     
    <!-- Header -->
    <div class="col-md-10">
    
       <br>
    </div>

 

</div>

